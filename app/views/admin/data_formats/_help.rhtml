	  autoHeight: true,
	  defaults: {
		  bodyStyle: 'padding:1px'
	  },
	  layoutConfig: {
		  titleCollapse: false,
		  animate: true
	  },
	  items: [{ title: 'Help', contentEl:'panel-help'},
			  { title: 'Meta Char', contentEl:'panel-meta'},
			  { title: 'Escapes', contentEl:'panel-escapes'},
			  { title: 'Classes', contentEl:'panel-classes'},
			  { title: 'PrintF', contentEl:'panel-printf' }]
	 });
});
</script>