<form id='search' action="/inventory/compounds/search" onsubmit="GetStructure();" method="post">
<object id="MolEditor" 
        codebase="MDL.Draw.Editor.dll#-1,-1,-1,-1" 
	height="150" 
	width="150" 
	classid="CLSID:FCE57399-E34B-45ce-881B-5FDFF3583307">
<PARAM NAME="BackColor" VALUE="LightBlue">
<param name="ChimeString" value="<%=@query_chime%>">
</object>
<br />
LU number:<br/> 
<input id="name" name="name" value='<%=@query_id%>' type="text" /><br />
<input id='chime' name="chime" type="hidden"></input>
<br />
Search Style 
<br/>
<select id='type' name='type'>
  <option>LU_Number</option>
  <option>sss</option>
  <option>match</option>
  <option>sim</option>
</select>
<input name="commit" type="submit" value="Find" />
</form>



<script language="javascript">

  function GetStructure(){
        var molEditor = document.getElementById('MolEditor');
        var field = document.getElementById('chime');
        field.value = molEditor.ChimeString();
	
  }
</script>

